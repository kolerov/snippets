# Use these for ARCv2:
#
#     set target_alias    "arc-elf32"
#     set target_triplet  "arc-unknown-elf32"
#
# Use these for ARCv3:
#
#     set target_alias    "arc64-elf"
#     set target_triplet  "arc64-unknown-elf"

set target_alias    "arc-elf32"
set target_triplet  "arc-unknown-elf32"
set tmpdir          "tmp"

# For Newlib
set tool_version    "4.3.0"

set verbose 0
